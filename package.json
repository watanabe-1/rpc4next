{
  "name": "rpc4next-monorepo",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/watanabe-1/rpc4next.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "bun --filter \"./packages/*\" --if-present build",
    "clean": "bun --filter \"./packages/*\" --if-present clean",
    "typecheck": "bun --filter \"./packages/*\" --if-present typecheck",
    "lint": "bun --filter \"./packages/*\" --if-present lint",
    "lint:fix": "bun --filter \"./packages/*\" --if-present lint:fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage.enabled true",
    "test:ui": "vitest --ui --coverage.enabled true",
    "test:watch": "vitest --watch",
    "diff2prompt": "diff2prompt --excludeFile=.gitignore"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.9",
    "@vitest/ui": "^4.0.18",
    "diff2prompt": "^0.0.8",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.4.1",
    "fast-glob": "^3.3.3",
    "mock-fs": "^5.5.0",
    "msw": "^2.12.10",
    "next": "16.1.6",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  }
}
